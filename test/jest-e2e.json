{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": "..",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "^src/(.*)$": "<rootDir>/src/$1",
    "@app/api-doc/(.*)": "<rootDir>/libs/common/api-doc/$1",
    "@app/api-doc": "<rootDir>/libs/common/api-doc",
    "@app/cashe/(.*)": "<rootDir>/libs/shared/cashe/src/$1",
    "@app/cashe": "<rootDir>/libs/shared/cashe/src"
  }
}